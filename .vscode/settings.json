{
    "files.readonlyInclude": {
        "**/Cargo.lock": true,
    },
    "editor.formatOnSave": true,
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "clippy",
        "--fix",
        "--workspace",
        "--message-format=json-diagnostic-rendered-ansi",
        "--all-targets",
        "--allow-dirty"
    ],
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
    },
}